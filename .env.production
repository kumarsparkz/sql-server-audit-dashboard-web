# ============================================================================
# PRODUCTION ENVIRONMENT VARIABLES (.env.production)
# ============================================================================

# API Configuration - Production
REACT_APP_API_BASE_URL=https://your-production-api.com
REACT_APP_SIGNALR_HUB_URL=https://your-production-api.com/dashboardHub

# Production specific settings
REACT_APP_ENABLE_DEBUG_LOGS=false
REACT_APP_ENABLE_MOCK_DATA=false

# Standard refresh intervals for production
REACT_APP_DASHBOARD_REFRESH_INTERVAL=30000
REACT_APP_ALERTS_REFRESH_INTERVAL=10000

# Production optimizations
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false
